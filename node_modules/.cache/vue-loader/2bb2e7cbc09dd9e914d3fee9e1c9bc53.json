\r\n            <input type=\"search\" class=\"mui-input-clear\" placeholder=\"请输入搜索内容\" v-model=\"keyword\">\r\n        </div>\r\n        <ul class=\"mui-table-view\">\r\n            <li class=\"mui-table-view-cell mui-collapse\">\r\n                <a class=\"mui-navigate-right\" href=\"#\" @click=\"isShow\">筛选</a>\r\n                <div class=\"mui-collapse-content\">\r\n                    <form class=\"mui-input-group\">\r\n                        <div class=\"mui-input-row\">\r\n                            <label>最低价格</label>\r\n                            <input type=\"text\" placeholder=\"最低价格\" v-model=\"low\">\r\n                        </div>\r\n                        <div class=\"mui-input-row\">\r\n                            <label>最高价格</label>\r\n                            <input type=\"text\" placeholder=\"最高价格\" v-model=\"high\">\r\n                        </div>\r\n                        <div class=\"mui-button-row\">\r\n                            <button class=\"mui-btn mui-btn-primary\" type=\"button\" @click=\"handleSearch\">确认</button>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </li>\r\n        </ul>\r\n        <ul class=\"mui-table-view app-search-list\">\r\n            <li class=\"mui-table-view-cell\" v-for=\"item of list\">{{item.lname}}</li>\r\n        </ul>\r\n    </div>\r\n</template>\r\n<script>\r\n    export default {\r\n        data(){\r\n            return {\r\n                keyword:'',\r\n                low:0,\r\n                high:10000000,\r\n                show:false,\r\n                list:[]\r\n            }\r\n        },\r\n        methods:{\r\n            handleSearch(){\r\n                var kw=this.keyword;\r\n                var low=this.low;\r\n                var high=this.high;\r\n                if(kw==''){\r\n                    this.$toast('请输入内容');\r\n                    return;\r\n                }\r\n                this.$http.get('search?keyword='+kw+'&low='+low+'&high='+high).then(res=>{\r\n                    if(res.body.code==1){\r\n                        this.list=res.body.data;\r\n                        this.keyword='';\r\n                    }else{\r\n                        this.$toast(res.body.msg);\r\n                        this.keyword='';\r\n                    }\r\n                })\r\n            },\r\n            isShow(){\r\n                var box=document.querySelector('.app-search .mui-collapse-content');\r\n                if(this.show==false){\r\n                    box.style.display='block';\r\n                    this.show=true;\r\n                }else{\r\n                    box.style.display='none';\r\n                    this.show=false;\r\n                }\r\n            }\r\n        }\r\n    }\r\n</script>\r\n<style scoped>\r\n    .app-search .mui-input-row {\r\n        margin-top: 1rem;\r\n    }\r\n    .app-search .app-search-list .mui-table-view-cell {\r\n        background-color: #eee;\r\n        font-size: 1rem; \r\n    }\r\n    .app-search .app-search-icon {\r\n        position: absolute;\r\n        top: 0.3rem;\r\n        right: 0.5rem;\r\n    }\r\n</style>"]}]}